<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="tr.com.fdd.dto">
	<class name="TSubeDTO" table="t_sube">
	
	<id name="sId" column="sb_id" type="int" unsaved-value="null">
			<generator class="increment" />
		</id>
		<property name="sAd" column="sb_ad" type="java.lang.String" />
		<property name="sAdres" column="sb_adres" type="java.lang.String" />
		<property name="sTel" column="sb_tel" type="java.lang.String" />		
		<property name="sDurum" column="sb_durum" type="java.lang.String" />
		

	</class>
</hibernate-mapping>


