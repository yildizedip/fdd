<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="tr.com.fdd.dto">
	<class name="TAnketDTO" table="t_anket">
		<id name="id" column="id" type="int" unsaved-value="null">
			<generator class="increment" />
		</id>
		<property name="hastaId" column="hasta_id" type="int" />
		<property name="tarih" column="tarih" type="java.util.Date" />
		<property name="saat" column="saat" type="java.lang.String" />
		<property name="aciklama" column="aciklama" type="java.lang.String" />
		<property name="arayanKisi" column="arayan_kisi" type="int" />
		<property name="kayitTarihi" column="kayit_tarihi" type="java.util.Date" />
		<property name="durum" column="durum" type="java.lang.String" />
		<property name="memnuniyetDerecesi" column="memnuniyet_der" type="java.lang.String" />
	</class>
</hibernate-mapping>