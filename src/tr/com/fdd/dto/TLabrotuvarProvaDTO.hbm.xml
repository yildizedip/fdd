<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="tr.com.fdd.dto">
	<class name="TLabrotuvarProvaDTO" table="t_labratuvar_prova">
		<id name="id" column="id" type="int" unsaved-value="null">
			<generator class="increment" />
		</id>
		<property name="labIslemId" column="lab_islem_id" type="int" />
		<property name="provaTip" column="prova_tip" type="int" />
		<property name="provaTar" column="prova_tar" type="java.util.Date" />
		<property name="durum" column="durum" type="java.lang.String" />
	</class>
</hibernate-mapping>