<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="tr.com.fdd.dto">
	<class name="TMenuDTO" table="t_menu">
	
	<id name="id" column="id" type="int" unsaved-value="null">
			<generator class="increment" />
		</id>
		<property name="ustMenuId" column="ust_menu_id" type="int" />
		<property name="menuAd" column="menu_ad" type="java.lang.String" />
		<property name="menuPage" column="menu_page" type="java.lang.String" />		
		<property name="durum" column="durum" type="java.lang.String" />
		

	</class>
</hibernate-mapping>


