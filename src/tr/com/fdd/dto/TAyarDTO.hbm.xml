<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="tr.com.fdd.dto">
	<class name="TAyarDTO" table="t_ayarlar">
		<id name="id" column="id" type="int" unsaved-value="null">
			<generator class="increment" />
		</id>
		<property name="name" column="name" type="java.lang.String" />
		<property name="value" column="value" type="java.lang.String" />
		<property name="state" column="state" type="java.lang.String" />
		<property name="subeId" column="sube_id" type="int" />
	</class>
</hibernate-mapping>