<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="tr.com.fdd.dto">
	<class name="TKullaniciSubeDTO" table="t_kullanici_sube">	
	<id name="id" column="id" type="int" unsaved-value="null">
			<generator class="increment" />
		</id>
		<property name="kuId" column="ku_id" type="int" />
		<property name="sId" column="sube_id" type="int" />
		<property name="durum" column="durum" type="java.lang.String" />
		

	</class>
</hibernate-mapping>