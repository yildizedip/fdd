<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="tr.com.fdd.dto">
	<class name="TDepoSiparisUrunDTO" table="t_depo_siparis_urun">	
	<id name="id" column="id" type="int" unsaved-value="null">
			<generator class="increment" />
		</id>
		<property name="siparisId" column="siparis_id" type="int" />
		<property name="urunTipId" column="urun_tip_id" type="int" />
		<property name="depoFiyat" column="depo_fiyat" type="double" />
		<property name="subeFiyat" column="sube_fiyat" type="double" />
		<property name="adet" column="adet" type="int" />
		<property name="depoId" column="depo_id" type="int" />
		<property name="gondermeTar" column="gonderme_tar" type="java.util.Date" />
		<property name="eklemeTar" column="ekleme_tar" type="java.util.Date" />
		<property name="aciklama" column="aciklama" type="java.lang.String" />
		<property name="durum" column="durum" type="int" />

	</class>
</hibernate-mapping>