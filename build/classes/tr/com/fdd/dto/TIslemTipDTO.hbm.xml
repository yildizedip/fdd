<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="tr.com.fdd.dto">
	<class name="TIslemTipDTO" table="t_islem_tip" >
		<id name="id" column="ID" type="int" unsaved-value="null">
			<generator class="increment" />
		</id>
		
		<property name="ad" column="AD" type="java.lang.String" />
	    <property name="kod" column="KOD" type="java.lang.String" />
		<property name="durum" column="DURUM" type="java.lang.String" />
	
	
	</class>
</hibernate-mapping>