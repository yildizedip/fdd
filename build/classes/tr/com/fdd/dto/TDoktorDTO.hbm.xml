<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping package="tr.com.fdd.dto">
	<class name="TDoktorDTO" table="t_doktor">
		<id name="dId" column="d_id" type="int" unsaved-value="null">
			<generator class="increment" />
		</id>

		<property name="dAd" column="d_ad" type="java.lang.String" />
		<property name="dSoyad" column="d_soyad" type="java.lang.String" />
		<property name="dBrans" column="d_brans" type="java.lang.String" />
		<property name="dMemberId" column="d_member_id" type="int" />
		<property name="dDurum" column="d_durum" type="java.lang.String" />

	</class>
</hibernate-mapping>